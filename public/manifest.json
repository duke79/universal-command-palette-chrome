{
  "short_name": "React App",
  "name": "Universal Command Palette",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "512": "logo512.png",
    "192": "logo192.png",
    "48": "favicon.ico"
  },
  "manifest_version": 2,
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ]
}
